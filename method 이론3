		Scanner sc = new Scanner(System.in);
		int a;
		int b;
		
		
		int[] array = new int[5];
		
		array[0]=1;
		array[1]=2;
		array[2]=3;
		array[3]=4;
		array[4]=5;
		
		
		
		System.out.print("첫번째 숫자를 입력해주세요");
		a = Integer.parseInt(sc.nextLine());
		System.out.print("두번째 숫자를 입력해주세요");
		b = Integer.parseInt(sc.nextLine());

		System.out.println("결과 값은 "+StringEx1(a,b));
		
		
		
		
		System.out.println("두번째 결과 값은 "+StringEx2(array));
		
	}


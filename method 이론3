	static int StringEx1(int a, int b) {
		
			int result=a+b;

			return result;
			
			
		}
	
	
	static int StringEx2(int[] array) {
		
		
		int result=0;

		for(int i =0; i<array.length;i++) {
			
			result=result+array[i];
			
			
		}
		

		return result;
		
		
	}


	public static void main(String[] args) {
		
//		String str = "바보야 너는 정말 병맛이야 지랄 좀 고만고만 해라";
